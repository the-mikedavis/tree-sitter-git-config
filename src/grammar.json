{
  "name": "git_config",
  "rules": {
    "config": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "section"
      }
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section_header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\r?\\n"
              },
              {
                "type": "SYMBOL",
                "name": "variable"
              }
            ]
          }
        }
      ]
    },
    "section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "section_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "\""
                },
                {
                  "type": "SYMBOL",
                  "name": "subsection_name"
                },
                {
                  "type": "STRING",
                  "value": "\""
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "section_name": {
      "type": "PATTERN",
      "value": "[\\w\\.]+"
    },
    "subsection_name": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\r\\n\\x00\\\"\\\\]+"
          },
          {
            "type": "SYMBOL",
            "name": "_escape_sequence"
          }
        ]
      }
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "name"
                },
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "FIELD",
                  "name": "value",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_value"
                  }
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "name"
            }
          ]
        }
      ]
    },
    "name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][\\w\\-]*"
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_boolean"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "_boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "true"
              },
              {
                "type": "STRING",
                "value": "yes"
              },
              {
                "type": "STRING",
                "value": "on"
              }
            ]
          },
          "named": true,
          "value": "true"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "false"
              },
              {
                "type": "STRING",
                "value": "no"
              },
              {
                "type": "STRING",
                "value": "off"
              }
            ]
          },
          "named": true,
          "value": "false"
        }
      ]
    },
    "true": {
      "type": "STRING",
      "value": "true"
    },
    "false": {
      "type": "STRING",
      "value": "false"
    },
    "integer": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "\\d+[kmgtpezyKMGTPEZY]?"
        }
      }
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_unquoted_string"
        }
      ]
    },
    "_quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\\"\\\\]"
              },
              {
                "type": "SYMBOL",
                "name": "_escape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_unquoted_string": {
      "type": "PATTERN",
      "value": "[^\\r\\n;# \\t\\f\\v][^\\r\\n;#]*"
    },
    "_escape_sequence": {
      "type": "PATTERN",
      "value": "\\\\([btnfr\"\\\\]|u[0-9a-fA-F]{4}|U[0-9a-fA-F]{8})"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[#;]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\\r\\n]+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t\\f\\v]"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

